{
  "name": "aformjs",
  "version": "1.0.0",
  "description": "aformjs is a fast json-schema form generator utilising a fork of hyperapp and futilsjs.",
  "main": "./src/index.js",
  "scripts": {
    "start:data": "xcopy data\\*.json dist\\ /Y",
    "start:parcel": "parcel ./src/index.html",
    "start:components": "parcel ./src/components/_test.html",
    "start": "npm run start:components",
    "watch": "parcel watch ./src/index.html"
  },
  "author": "jklarenbeek@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jklarenbeek/aformjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jklarenbeek/aformjs.git"
  },
  "keywords": [
    "futilsjs",
    "json",
    "schema",
    "jsonschema",
    "bulma",
    "hyperapp"
  ],
  "files": [
    "src/",
    "_lib/"
  ],
  "alias": {
    "aformjs": "./src"
  },
  "engines": {
    "node": ">= 10.12.0",
    "npm": ">= 6.4.1"
  },
  "browserslist-note": "https://github.com/browserslist/browserslist-example",
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ],
    "production": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 and_chr version",
      "last 2 edge version",
      "last 2 safari version",
      "last 2 and_uc version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "postcss-modules": "^1.4.1",
    "postcss-normalize": "^7.0.1",
    "postcss-preset-env": "^6.6.0",
    "sass": "^1.20.1"
  },
  "dependencies": {
    "futilsjs": "^1.0.0"
  },
  "sideEffects": false,
  "postcss": {
    "modules": true,
    "plugins": {
      "postcss-normalize": {},
      "postcss-preset-env": {},
      "autoprefixer": {}
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "useBuiltIns": false,
          "debug": true
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "h"
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ],
    "ignore": [
      "node_modules",
      "_old"
    ]
  },
  "eslintIgnore": [
    "dist/*",
    "_old/*",
    "_lib/*",
    "_doc/*"
  ],
  "$comment": "dude, ... dont ask me, I dont understand this stuff.",
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "extends": [
      "airbnb-base"
    ],
    "env": {
      "node": true,
      "browser": true
    },
    "plugins": [
      "compat",
      "html",
      "import",
      "react"
    ],
    "rules": {
      "no-unused-vars": [
        2,
        {
          "varsIgnorePattern": "h"
        }
      ],
      "react/jsx-uses-vars": 2,
      "no-prototype-builtins": 0,
      "no-continue": 0,
      "prefer-template": 0,
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "no-restricted-syntax": 0,
      "no-else-return": 0,
      "no-underscore-dangle": 0,
      "wrap-iife": [
        "error",
        "inside"
      ],
      "prefer-spread": 0,
      "guard-for-in": 0,
      "prefer-rest-params": 0,
      "dot-notation": 0,
      "camelcase": 0,
      "func-names": [
        "error",
        "never"
      ],
      "class-methods-use-this": "off",
      "max-len": 0,
      "lines-between-class-members": 0,
      "function-paren-newline": 0,
      "prefer-destructuring": 0,
      "no-plusplus": 0,
      "global-require": 0,
      "import/no-unresolved": 0,
      "no-param-reassign": 0,
      "no-shadow": 0,
      "import/extensions": 0,
      "import/newline-after-import": 0,
      "no-multi-assign": 0,
      "no-nested-ternary": 0,
      "no-mixed-operators": 0
    }
  }
}
